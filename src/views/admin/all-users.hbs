{{#if users}}
<div class="container p-5">
    <h4 class="h10w">- Gesti√≥n de Usuarios -</h4>
    <br>
    <div class="rowhey">
        <div class="huju">
            {{#each users}}
            <ul class="list-group">
                <li class="list-group-item colorfondopanel bordertop">
                    <div class="thumbnail3 ">
                    <h4 class="titulos">Nombre: {{this.nombre}}</h4>
                    {{#operaciones this.email '===' "admin@gmail.com"}}
                    <img class=" img-responsive allbordersgris" src="https://fiverr-res.cloudinary.com/images/q_auto,f_auto/gigs/125568526/original/cd9c93141521436a112722e8c5c0c7ba0d60a4a2/be-your-telegram-group-admin.jpg" alt="...">
                    {{else}}
                     <img class=" img-responsive allbordersgris" src="data:image/png;base64," alt="...">
                     {{/operaciones}}
                    <br>
                    <div class="h5w">Apellido: {{this.apellido}}</div>
                    <br>
                    <div class="h5w">Calle: {{this.calle}}</div>
                    <br>
                    <div class="h5w">Tlf: {{this.tlf}}</div>
                    <br>
                    <div class="h5w">Email: {{this.email}}</div>
                    <br><br>
                    {{#operaciones this.email '===' "admin@gmail.com"}}
                    {{else}}
                    <form action="/users/delete/{{_id}}?_method=DELETE" method="POST">
                        <input type="hidden" name="_method" value="DELETE">
                        <button type="submit" class="btn  negro buttonb allbordersgris borde30">Eliminar</button>
                    </form>
                    {{/operaciones}}
                    </div>
                </li>
                <hr>
            </ul>
            {{/each}}
        </div>
    </div>
    <br><br>
    {{else}}
    <br><br><br><br><br>
    <div class="empty-cart">
        <h1>No hay Usuarios disponibles</h1>
    </div>
</div>
</div>
{{/if}}